<%# LOCALS: index, section_index, admin %>
<div id="session-<%= index %>" class="session session-<%= index %>  col-xs-12" data-session_index="<%= index %>">
   <h5 class="section-header hidden">Sections</h5>
   <div class="sections">
     <%= render :partial => 'shared/forms/section_block',
                :locals => {
                :session_i => index,
                :section_index => section_index,
                :admin => admin} %>
   </div> <!-- END .sections -->
   <div class="add_section-button-block">
     <button class="add_section btn btn-default">Add Section</button>
     <span class="help-block">Sections are subgroups of a class that will meet at different times but consult the same materials.</span>
   </div>
</div> <!-- END .session -->
